msgctxt "MSG_Misc_Prepend (//)"
msgid "_Prepend \"http://\" to URLs without a scheme"
msgstr "F_oranstille \"http://\" på URL'er uden protokol"

msgctxt "MSG_Misc_Prepend_Help (//)"
msgid ""
"If selected, \"http://\" is added to any URL\n"
"without a scheme, so that it is opened in the\n"
"browser."
msgstr "Sættes hak her, bliver der sat \"http://\" foran enhver URL\nsom ikke i forvejen har en protokol angivet, så den åbnes\ni browseren."

msgctxt "MSG_Misc_UseMailer (//)"
msgid "S_end mailto: URLs to email application"
msgstr "S_ende \"mailto:\"-URL'er til e-postprogrammet"

msgctxt "MSG_Misc_UseMailer_Help (//)"
msgid ""
"If selected, \"mailto:\" URLs are sent to\n"
"the mailer, otherwise they are sent to the\n"
"browser."
msgstr "Sættes hak her, bliver \"mailto:\"-URL'er sendt\ntil e-postprogrammet, ellers sendes de til\nbrowseren."

msgctxt "MSG_Misc_UseFTP (//)"
msgid "Se_nd \"ftp://\" URLs to FTP application"
msgstr "Se_nde \"ftp://\"-URL'er til FTP-klienten"

msgctxt "MSG_Misc_UseFTP_Help (//)"
msgid ""
"If selected, \"ftp://\" URLs are sent to\n"
"the FTP client, otherwise they are sent to\n"
"the browser."
msgstr "Sættes hak her, blvier \"ftp://\"-URL'er\nsendt til FTP-klienten, ellers sendes de\ntil browseren."

msgctxt "MSG_Misc_Show (//)"
msgid "Un_iconify application"
msgstr "Af_ikonificere applikationen"

msgctxt "MSG_Misc_Show_Help (//)"
msgid ""
"If selected, the application is uniconified,\n"
"before sending the URL to it."
msgstr "Sættes hak her, bliver applikationen\nafikonificeret før URL'en sendes til den."

msgctxt "MSG_Misc_Bring (//)"
msgid "_Bring browser screen to front"
msgstr "_Lægge browserens skærm i forgrunden"

msgctxt "MSG_Misc_Bring_Help (//)"
msgid ""
"If selected, the application screen is moved\n"
"to front, before sending the URL."
msgstr "Sættes hak her, vil applikationens skærm blive\nflyttet så den ligger forrest, før URL'en sendes til den."

msgctxt "MSG_Misc_Open (//)"
msgid "_Open URL in new client window (or tab)"
msgstr "_Åbne URL i nyt klientvindue (eller faneblad)"

msgctxt "MSG_Misc_Open_Help (//)"
msgid ""
"If selected, a new application window (or tab)\n"
"is opened, before sending the URL."
msgstr "Sætets hak her, vil der blive åbnet et nyt vindue\n(eller faneblad) i klienten, før URL'en sendes."

msgctxt "MSG_Misc_Launch (//)"
msgid "_Launch browser if one isn't running"
msgstr "S_tarte browser hvis den ikke kører"

msgctxt "MSG_Misc_Launch_Help (//)"
msgid "If selected, a new application is run, if none is found."
msgstr "Sættes hak her, vil applikationen blive startet\nhvis den ikke findes kørende."

msgctxt "MSG_About_WinTitle (1100//)"
msgid "About OpenURL"
msgstr "Om OpenURL"

msgctxt "MSG_About_OK (1111//)"
msgid "_OK"
msgstr "_OK"

msgctxt "MSG_About_Descr (1112//)"
msgid ""
"\\33bOpenURL\\33n is a shared library for easily sending\n"
"URLs from applications to browsers, mailers\n"
"and FTP clients.\n"
"\n"
"\\33cOpenURL is a Public Domain Software."
msgstr "\\33bOpenURL\\33n er et systembibliotek til enkelt at\nsende URL'er fra applikationer til browsere, e-postprogrammer\nog FTP-klienter.\n\n\\33cOpenURL er Public Domain-software."
